@model List<ChurchPlannerApp.Models.Song>
@{
    ViewBag.Title = "Songs";
}

<div class="container">
    <div class="row">
        <div class="col-sm-8 col-sm-offset-2">
                @foreach (var m in Model)
            {
                    <a href="@m.Url" target="new">@m.Title</a>
                    <br /> 

                }
        </div>
    </div>
</div>
